<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.example_internet.MainActivity">



    <Button

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="打开webview"
        android:onClick="onClickWebView"
        android:background="@drawable/custom_button"/>

    <Button

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="HttpURLConnection"
        android:onClick="onClickHttpURLConnection"
        android:background="@drawable/custom_button"/>

    <Button

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="OkHttp"
        android:onClick="onClickOkHttp"
        android:background="@drawable/custom_button"/>

    <Button

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="PullXml"
        android:onClick="onClickPullXml"
        android:background="@drawable/custom_button"/>


    <Button

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SAXXML"
        android:onClick="onClickSAXXml"
        android:background="@drawable/custom_button"/>

    <Button

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="JSONObject"
        android:onClick="onClickJSONObject"
        android:background="@drawable/custom_button"/>

    <Button

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Gson"
        android:onClick="onClickGSON"
        android:background="@drawable/custom_button"/>




</LinearLayout>
