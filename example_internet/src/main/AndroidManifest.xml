<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.example_internet">

    <uses-permission android:name="android.permission.INTERNET" />
    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".webview.WebViewActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name=".TestHttpURLActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name=".TestOkHttp"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name=".TestPullXml"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name=".saxxml.SAXXmlActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name=".json.JSONObjectActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>

        <activity android:name=".json.GsonActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>


    </application>

</manifest>